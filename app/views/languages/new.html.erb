<% provide(:title, "New Language")%>
<h1>languages#new</h1>
<h1>Add new Language here</h1>
<div>
	<%= form_for :language, url: languages_path do |lang| %>
		<%= lang.label :language %>
		<%= lang.text_field :language %>
		<%= lang.submit class: "btn btn-success" %>
	<% end %>
</div>
<%= link_to "Home", root_path, class: "btn btn-info" %> <%= link_to "List Languages", languages_path, class: "btn btn-info" %>